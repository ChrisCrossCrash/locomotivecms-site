{% extends "layouts/base" %}

{% block main %}
  <header class="header-primary">
    <div class="grid grid-centered">
      <div class="grid-cell">
        {% include navigation %}
        {% block header %}
        <div class="header-content is-small">
          <h1>{% editable_text "headline", format: raw, rows: 1 %}Page not found{% endeditable_text %}</h1>
          <p>
            {% editable_text "tagline", format: raw, rows: 3 %}
            Sorry, but we didn't find the page you were looking for.
            {% endeditable_text %}
          </p>
        </div>
        {% endblock %}
      </div>
    </div>
  </header>
{% endblock %}
